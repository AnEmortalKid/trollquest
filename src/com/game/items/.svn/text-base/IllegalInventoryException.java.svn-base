package com.game.items;

/**
 * Used to designate something went wrong with the inventory when doing some
 * action to it
 * 
 * @author taylor
 * 
 */
public class IllegalInventoryException extends Exception {

  /**
	 * 
	 */
	private static final long serialVersionUID = 1984362191250693131L;
  

  /**
   * Creates a new exception that calls the super type {@link Exception} with
   * the specified message
   * 
   * @param string
   *          the error message
   */
  public IllegalInventoryException(String string) {
    super(string);
  }


}
